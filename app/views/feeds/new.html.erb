<%= form_for @feed, url: {action: 'create'} do |f| %>
  <fieldset>
    <p class="lead"/>
    <div class="field form-group">
      <%= f.text_field :link, :placeholder => 'http(s)://example.com/rss', :class => 'form-control' %>
    </div>
    <div class="actions">
      <%= f.submit "Add", :class => 'btn btn-primary' %>
    </div>
  </fieldset>
<% end %>
